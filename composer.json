{
    "name": "worksome/foggy-laravel",
    "description": "Adds Foggy support in Laravel",
    "type": "library",
    "require": {
        "php": "^8.2",
        "worksome/foggy": "^0.6",
        "illuminate/support": "^10.0 || ^11.0",
        "illuminate/console": "^10.0 || ^11.0"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Worksome\\FoggyLaravel\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Worksome\\FoggyLaravel\\FoggyServiceProvider"
            ]
        }
    }
}
